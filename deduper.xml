<?xml version="1.0"?>
<Container version="2">
  <Name>Deduper</Name>
  <Repository>daman20/deduper</Repository>
  <Registry>https://hub.docker.com/repository/docker/daman20/deduper</Registry>
  <Network>bridge</Network>
  <MyIP/>
  <Shell>sh</Shell>
  <Privileged>false</Privileged>
  <Support>https://forums.unraid.net/topic/110500-support-daman20-deduper/</Support>
  <Project>https://github.com/daman20/deduper</Project>
  <Overview>Deduper is a Python script bundled into a Docker container that automatically deletes any files with the same content, regardless of name. This is done based off of the SHA512 hash.</Overview>
  <Category>Backup:</Category>
  <WebUI>http://[IP]:[PORT:5800]</WebUI>
  <TemplateURL/>
  <Icon>https://github.com/daman20/deduper/blob/862ca5765e6762e9499d1b35ed6c6f7d43496cb2/Deduper%20Logo.png</Icon>
  <ExtraParams/>
  <PostArgs/>
  <CPUset/>
  <DateInstalled>1624675401</DateInstalled>
  <DonateText/>
  <DonateLink/>
  <Description>Deduper is a Python script bundled into a Docker container that automatically deletes any files with the same content, regardless of name. This is done based off of the SHA512 hash.</Description>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort/>
        <ContainerPort>5800</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data>
    <Volume>
      <HostDir/>
      <ContainerDir>/scan</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Environment/>
  <Labels/>
  <Config Name="Scan Directory" Target="/scan" Default="" Mode="rw" Description="The directory for the program to dedupe." Type="Path" Display="always" Required="true" Mask="false"/>
  <Config Name="WebUI" Target="5800" Default="5800" Mode="tcp" Description="The port of the WEbUI" Type="Port" Display="always" Required="true" Mask="false"/>
</Container>
